        "**/tests/**",
        "**/storage/framework/views/**",
        "**/vendor",
        "_ide_helper.php",
        "_ide_helper_models"
    ],
    "editor.renderWhitespace": "all",
    "files.trimTrailingWhitespace": true,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "files.eol": "\n"
